		#sidebar {
			float: left;
			margin-left: 5vw; 
			margin-right: 10px;
			width: 15vw; 
			height: 40vh; 
			padding: 1vw;
			background-color: #117F44; 
			color: white; 
			font-size: x-large; 
			border-radius: 10px 10px 10px 10px; 
		}

		.side:hover {
			background-color: #15B743; 
			cursor: pointer; 
		}

		
		#navbar {
            margin-left: 20%;
			overflow: hidden;
			width: 70vw; 
			height: 4vh;
			font-size: calc(5px + 1vw); 
		}

		.nav {
			display: inline-block;
			background-color: #117F44; 
			width: calc(12px + 14%); 
			height: 4vh; 
			text-align: center;  
			line-height: 4vh;
			transition: width 1s; 
			color: white; 
			border-radius: 10px 10px 0px 0px; 
		}

		/* .nav:hover {
			background-color: #15B743; 
			width: calc(18px + 18%);
			cursor: pointer;   
		} */

		#main-content {
			border-radius: 0px 10px 10px 10px; 
			overflow: hidden;
			margin-left: 20%; 
			margin-right: 1vw; 
			width: 60%; 
			padding: 10px;
			background-color: #15B743; 
			color: white; 
		}

		#content-table {
			width: 100%; 
		}

        .contentDiv {
			display: inline-block; 
			margin-right: 2vw; 
			margin-bottom: 2vh;
			margin-top: 2vh;
		}

		.studentData {
			display: inline-block; 
			margin-left: 2vw; 
		}

		#rightContent {
			float: right; 
			margin-right: 10vw; 
		}

		button:hover {
			cursor: pointer;
		}
    </style>

</head>

